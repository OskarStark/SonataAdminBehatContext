{
    "name": "oskarstark/sonata-admin-behat-context",
    "description": "a Behat Context for SonataAdmin",
    "keywords": ["BDD", "Behat", "Symfony2", "Context", "Sonata-Admin", "Sonata"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oskar Stark",
            "email": "oskarstark@googlemail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "behat/behat": "^3.0",
        "behat/mink-extension": "^2.3",
        "behat/symfony2-extension": "^2.1",
        "sonata-project/admin-bundle": "^3.0"
    },
    "require-dev": {
        "behat/mink-goutte-driver": "^1.1",
        "guzzlehttp/guzzle": "^6.3",
        "behat/mink-selenium2-driver": "^1.3",
        "phpunit/phpunit": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.11"
    },
    "autoload": {
        "psr-4": {
            "OStark\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\OStark\\": "tests/"
        }
    }
}
