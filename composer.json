{
    "name": "oskarstark/sonata-admin-behat-context",
    "type": "library",
    "description": "a Behat Context for SonataAdmin",
    "keywords": [
        "BDD",
        "Behat",
        "Symfony2",
        "Context",
        "Sonata-Admin",
        "Sonata"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Oskar Stark",
            "email": "oskarstark@googlemail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "behat/behat": "^3.0",
        "behat/mink-extension": "^2.3",
        "friends-of-behat/symfony-extension": "^2.1"
    },
    "require-dev": {
        "behat/mink-goutte-driver": "^1.1",
        "behat/mink-selenium2-driver": "^1.3",
        "guzzlehttp/guzzle": "^6.3",
        "phpunit/phpunit": "^6.0",
        "sonata-project/user-bundle": "^4.0",
        "symfony/browser-kit": "^4.3"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "OStark\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\OStark\\": "tests/"
        }
    },
    "suggest": {
        "sonata-project/user-bundle": "Allows User & Authentication scenarios"
    }
}
